@model IEnumerable<PPI.Core.Domain.Entities.vDashboard>
@{
    
}
<p class="bold">People with all (3) assessments 'Incomplete'</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(m => m.FirstOrDefault().Person.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(m =>  m.FirstOrDefault().Person.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(m => m.FirstOrDefault().Person.Gender)
        </th>
        <th>
            @Html.DisplayNameFor(m => m.FirstOrDefault().Person.AAMCNumber)
        </th>
        <th>
            @Html.DisplayNameFor(m => m.FirstOrDefault().Person.PrimaryEmail)
        </th>
        <th>
            @Html.DisplayNameFor(m => m.FirstOrDefault().Person.Hogan_Id)
        </th>
        <th>@Html.DisplayNameFor(m => m.FirstOrDefault().HPIDate)</th>
        <th>@Html.DisplayNameFor(m => m.FirstOrDefault().HDSDate)</th>
        <th>@Html.DisplayNameFor(m => m.FirstOrDefault().MVPIDate)</th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Person.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Person.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Person.Gender)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Person.AAMCNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Person.PrimaryEmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Person.Hogan_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HPIDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HDSDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MVPIDate)
            </td>
            <td></td>
        </tr>
    }
</table>
@Html.ActionLink("Export", "Export", new { @eventId = (int)ViewData["EventId"], @assessmentStatus = "UnCompleted" }, new { @class = "btn btn-primary btn200 pull-right" })

