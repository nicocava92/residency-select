<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge;chrome=1" />
    <title>@ViewBag.Title - Residency Select</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/csstheme")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/spin")
    @if (IsSectionDefined("AddToHead"))
    {
        @RenderSection("AddToHead", required: false)
    }

    <!--Google Fonts-->
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400,600,700,300|Titillium+Web:200,300,400' rel='stylesheet' type='text/css'>

</head>
<body id="login-page-full">
    <div id="login-full-wrapper">
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <div id="login-box">
                        <div class="row">
                            <div class="col-xs-12">
                                <header id="login-header" class="plr20">
                                    <div id="login-logo">
                                        <img src="~/Content/Images/j3p_dots_portal.png" alt="" class="img-responsive" />                                        
                                    </div>
                                </header>
                                <div id="login-box-inner">
                                    @RenderBody()
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/bootstraptheme")
    @Scripts.Render("~/bundles/themeplugin")

    @RenderSection("scripts", required: false)
</body>
</html>

